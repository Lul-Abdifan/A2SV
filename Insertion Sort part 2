/*
 * Complete the 'insertionSort2' function below.
 *
 * The function accepts following parameters:
 *  1. INTEGER n
 *  2. INTEGER_ARRAY arr
 */

function insertionSort2(n, arr) 
{
    // Write your code here
    for(let i=1;i<n;i++)
    {
        let tmp = arr[i];
        let j = i - 1;
        while(j>=0 && arr[j]>tmp)
        {
            arr[j+1] = arr[j];
            j--;
        }
        arr[j+1] = tmp;
   
 console.log(...arr);  
   }
}
